{
	"compilerOptions": {
		"module": "esnext",
		"moduleResolution": "node",
		"target": "es5",
		"sourceMap": true,
		"allowJs": true,
		"strict": true,
		"noImplicitAny": false,
		"baseUrl": ".",
	  	"paths": {
			"@src/*": ["./src/*"],
			"@shared/*": ["./shared/*"]
	  	}
	},
	"include": [
		"src/**/*",
		"shared/**/*",
	]
}